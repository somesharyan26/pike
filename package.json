{
  "name": "ola-suvidha",
  "title": "Ola Suvidha",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "export HOST=0.0.0.0; PORT=9100 node ./bin/www",
    "prod:start": "export NODE_ENV=production; PORT=9100 nodemon ./bin/www",
    "dev:start": "export NODE_ENV=development; PORT=9100 nodemon ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf public/* && webpack -p",
    "dev:build": "export NODE_ENV=development; npm run build",
    "prod:build": "export NODE_ENV=production; npm run build",
    "debug": "export NODE_ENV=development; PORT=9100 nodemon --inspect --debug-brk ./bin/www",
    "quickstart": "npm run build; npm start",
    "dev:quickstart": "export NODE_ENV=development; npm run quickstart",
    "prod:quickstart": "export NODE_ENV=production; npm run quickstart",
    "webserver": "export NODE_ENV=development; webpack-dev-server --port 9000 --progress --colors --inline --hot --host 0.0.0.0 --content-base public/",
    "dev": "concurrently 'npm run dev:start' 'npm run webserver'",
    "task": "node ./server/tasks/taskRunner"
  },
  "author": "Saurabh Kumar Dixit",
  "license": "ISC",
